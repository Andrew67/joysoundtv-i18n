{
  "manifest_version": 2,
  "name": "JoySound.TV Translator",
  "version": "1.0",
  "author": "Andrew67 Software",
  "homepage_url": "https://github.com/Andrew67/joysoundtv-i18n",
  "description": "Translates the PS3 KyokuNavi to English. karaoke by Becris from the Noun Project",
  "content_scripts": [
    {
      "matches": [
        "https://vt.joysound.tv/kyokunavi/ps3/*"
      ],
      "js": [
        "domUtil.js",
        "joysoundtv-i18n-ps3-en.user.js"
      ],
      "css": [
        "joysoundtv-i18n-ps3-en.user.css"
      ]
    }
  ],
  "icons": {
    "48": "noun_975688_cc-48.png",
    "64": "noun_975688_cc-64.png",
    "96": "noun_975688_cc-96.png"
  },
  "applications": {
    "gecko": {
      "id": "joysoundtv-i18n@andrew67.com",
      "strict_min_version": "57.0"
    }
  }
}
